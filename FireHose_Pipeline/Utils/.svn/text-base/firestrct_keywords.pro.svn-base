;; The sole purpose of this function is to contain all of the common
;; keywords associated with the firestrct functions.
;; All of these firestrct functions (~10 in all) call this function and use
;; keyword inheritance, meaning that this is the only place that these
;; keywords have to be stored.

PRO firestrct_keywords,USE=use,RAWPATH=rawpath,FITSFILE=fitsfile,HDR=hdr,EXPTYPE=exptype,$
	OBJECT=object,AIRMASS=airmass, $
	SLIT=slit, EXPTIME=exptime,JD=jd,RA_DEG=ra_deg,DEC_DEG=dec_deg, $
	OBJ_ID=obj_id,SENSFUNC=sensfunc, $
	TELLFUNC=tellfunc,NUM_ARCS=num_arcs,ARCS=arcs,NUM_FLATS=num_flats,$
	FLATS=flats,NUM_ILLUMS=num_illums,ILLUMS=illums,ILLUMFLATFILE=illumflatfile, $
	PIXFLATFILE=pixflatfile, PIXIMGFILE=piximgfile, $
	ORDERFILE=orderfile, ORDR_STR_FILE=ordr_str_file, $
	ARCFITS=arcfits, ARCIMGFITS=arcimgfits, $
	ECHEXTFILE=echextfile, OBJSTRFILE=objstrfile, TECHEXTFILE=techextfile, $
	TOBJSTRFILE=tobjstrfile, POS_SLIT=pos_slit, $
	TELLNAME=tellname,NUM_TFILES=num_tfiles,TFILES=tfiles, $
	NUM_TARCS=num_tarcs,TARCS=tarcs,NUM_TFLATS=num_tflats,TFLATS=tflats,$
	TARCFITS=tarcfits, TARCIMGFITS=tarcimgfits,COMMENTS=comments, $
	WARNING=warning, MAGCAT_COMMENTS=magcat_comments, TBMAGS=tbmags, TVMAGS=tvmags, $
	OH_CAL=oh_cal

	RETURN
						
END
